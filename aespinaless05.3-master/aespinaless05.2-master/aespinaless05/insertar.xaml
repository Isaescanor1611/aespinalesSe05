<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="aespinaless05.insertar">
  <ContentView.Content>
      <StackLayout>
            <Label Text="INSERTAR ESTUDIANTE"
                FontSize="20"
                HorizontalTextAlignment="Center"/>
            <Entry x:Name="txtCodigo" Placeholder="Ingrese Codigo" Keyboard="Numeric"/>
            <Entry x:Name="txtNombre" Placeholder="Nombre"/>
            <Entry x:Name="txtApellido" Placeholder="Apellido"/>
            <Entry x:Name="txtEdad" Placeholder="Edad" Keyboard="Numeric"/>
            <Button x:Name="btnInsertar" Text="Insertar" Clicked="btnInsertar_Clicked"/>
        </StackLayout>
  </ContentView.Content>
</ContentView>